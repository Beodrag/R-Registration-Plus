{
    "name": "R'Registration Plus",
    "description": "Improves the UI of the UCR Registration Page and adds direct links to both the Course Difficulty Database and Rate My Professors",
    "version": "1.2",
    "manifest_version": 3,
    "content_scripts": [
        {
            "matches": ["*://*.registrationssb.ucr.edu/*"],
            "all_frames": true,
            "js": ["content.js"],
            "run_at": "document_end"
        }
    ],
    "icons": { "16": "16.png",
            "48": "48.png",
            "128": "128.png" },
    "action": {
        "default_icon": {
            "16": "16.png",
            "48": "48.png",
            "128": "128.png"
        }
    }
    
}